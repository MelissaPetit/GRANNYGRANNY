    <div class="container d-flex">
        <% @bookings.each do |booking| %>
                <div class="card me-5" style="width: 18rem;">
                <img src="" class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title">List my Booking</h5>
                    <p class="card-text"><%= booking.status %></p>
                    <p class="card-text"><%= booking.participant_number %></p>
                    <p class="card-text"><%= booking.date %></p>
                    <p class="card-text"><%= booking.comment %></p>
                    <p class="card-text"><%= booking.total_price %></p>
                    <button type="button" class="btn btn-primary text-white"><%= link_to "TODO:Delete my booking", root_path %></button>
               </div>
               </div>
        <% end %>
    </div>
